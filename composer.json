{
  "name": "coinsbank/rest-api-sdk-php",
  "description": "Coinsbank PHP SDK for REST-API",
  "keywords": [
    "coinsbank",
    "rest",
    "sdk"
  ],
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "https://github.com/coinsbank/rest-api-sdk-php",
  "authors": [
    {
      "name": "Coinsbank",
      "homepage": "https://github.com/coinsbank/rest-api-sdk-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Coinsbank\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "config": {
    "vendor-dir": "vendor",
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.phundament.com"
    }
  ]
}
